{
  "compilerOptions": {
    "strict": true,
    "noImplicitAny": true,
    "jsx": "react",
    "esModuleInterop": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "target": "es5",
    "module": "commonjs",
    "lib": ["es2015", "dom"]
  }
}
